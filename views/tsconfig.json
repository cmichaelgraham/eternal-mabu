{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./animation-main.ts",
        "./app.ts",
        "./child-router.ts",
        "./esri-map.ts",
        "./flickr.ts",
        "./main.ts",
        "./nav-bar.ts",
        "./welcome.ts"
    ]
}
